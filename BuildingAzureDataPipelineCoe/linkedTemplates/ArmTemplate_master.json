{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "BuildingAzureDataPipelineCoe"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataExplorer_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureDataExplorer'"
		},
		"AzureDataLakeStorage_Coe_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_Coe'"
		},
		"AzureDatabricks_Coe_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks_Coe'"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"FileServer2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer2'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"SqlServer_Coe_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_Coe'"
		},
		"AzureDataExplorer_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "93f33571-550f-43cf-b09f-cd331338d086"
		},
		"AzureDataExplorer_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "bfa63f32-f4fd-481c-8e1e-f03cf4d90033"
		},
		"AzureDataExplorer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Sample"
		},
		"AzureDataLakeStorage_Coe_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls2coe.dfs.core.windows.net"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Coe"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "harshavardhanreddy.kurakula@dxc.com"
		},
		"FileServer2_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\hkurakula2\\Downloads"
		},
		"FileServer2_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "harshavardhanreddy.kurakula@dxc.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataExplorer_servicePrincipalKey": {
						"value": "[parameters('AzureDataExplorer_servicePrincipalKey')]"
					},
					"AzureDataLakeStorage_Coe_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_Coe_accountKey')]"
					},
					"AzureDatabricks_Coe_accessToken": {
						"value": "[parameters('AzureDatabricks_Coe_accessToken')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"SqlServer_Coe_connectionString": {
						"value": "[parameters('SqlServer_Coe_connectionString')]"
					},
					"AzureDataExplorer_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataExplorer_properties_typeProperties_tenant')]"
					},
					"AzureDataExplorer_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataExplorer_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataExplorer_properties_typeProperties_database": {
						"value": "[parameters('AzureDataExplorer_properties_typeProperties_database')]"
					},
					"AzureDataLakeStorage_Coe_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_Coe_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"FileServer2_password": {
						"value": "[parameters('FileServer2_password')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					},
					"FileServer2_properties_typeProperties_host": {
						"value": "[parameters('FileServer2_properties_typeProperties_host')]"
					},
					"FileServer2_properties_typeProperties_userId": {
						"value": "[parameters('FileServer2_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		},
		{
			"name": "ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_1')]"
			]
		}
	]
}