				"annotations": [],
				"type": "AzureDataExplorer",
				"typeProperties": {
					"endpoint": "https://pocexplorer.southcentralus.kusto.windows.net",
					"tenant": "93f33571-550f-43cf-b09f-cd331338d086",
					"servicePrincipalId": "bfa63f32-f4fd-481c-8e1e-f03cf4d90033",
					"servicePrincipalKey": {
						"type": "SecureString",
						"value": "**********"
